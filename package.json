{
  "name": "react-ast",
  "version": "0.0.5",
  "description": "render abstract syntax trees with react",
  "keywords": [
    "abstract",
    "ast",
    "babel",
    "generate",
    "react",
    "reconcile",
    "reconciler",
    "render",
    "syntax",
    "template",
    "tree",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/codejamninja/react-ast",
  "bugs": {
    "url": "https://github.com/codejamninja/react-ast/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib",
    "bin"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/react-ast"
  },
  "scripts": {
    "build": "babel src -d lib --extensions \".ts,.tsx\" --source-maps inline && tsc -d --emitDeclarationOnly",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/* && jest --clearCache",
    "clean:purge": "jest --clearCache && git clean -fXd",
    "format": "prettier --write ./**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx} --ignore-path .gitignore",
    "lint": "npm run format && tsc --allowJs --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "npm run format && tsc --noEmit && eslint --ext .ts,.tsx --fix ./",
    "prepublish": "npm run build",
    "start": "babel-node --extensions \".ts,.tsx\" example",
    "test": "npm run lint && jest --coverage"
  },
  "dependencies": {
    "@babel/generator": "^7.5.5",
    "@babel/template": "^7.4.4",
    "@babel/types": "^7.5.5",
    "@types/node": "^12.7.2",
    "@types/react-reconciler": "^0.18.0",
    "core-js": "^3.2.1",
    "lodash": "^4.17.15",
    "npm-pkg-json": "^0.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-reconciler": "^0.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/babel-generator": "^6.25.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-preset-jam": "^0.4.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-jam": "^0.2.17",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
    "verbose": true
  }
}
