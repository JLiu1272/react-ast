{
  "name": "react-ast",
  "version": "0.1.30",
  "description": "render abstract syntax trees with react",
  "keywords": [
    "abstract",
    "ast",
    "babel",
    "generate",
    "react",
    "reconcile",
    "reconciler",
    "render",
    "syntax",
    "template",
    "tree",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/codejamninja/react-ast",
  "bugs": {
