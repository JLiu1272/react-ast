    "gnumake": "^0.1.0",
    "jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "((tests(/|/.*/)[^_/]*)|\\.spec)\\.[jt]sx?$",
